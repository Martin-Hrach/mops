<!DOCTYPE html>
<html>
<head>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.2/Chart.bundle.min.js"></script>
</head>
<body>
	<h1>Mops test-chart</h1>
	<canvas id="myChart" width="400" height="400"></canvas>

	<script>
	var lulWords = "<%= lulWords %>".split(",").map(a=> +a);
	var sadWords = "<%= sadWords %>".split(",").map(a=> +a);
	var timeArr = "<%= timeArr %>".split(",");
	var res = "<%= JSON.stringify(allResponse) %>"
	console.log(res)
	var ctx = document.getElementById("myChart").getContext('2d');
  var chart = new Chart(ctx, {
    type: 'line',
    data: {
      labels: timeArr,
      datasets: [
        {
          data: lulWords,
          label: "lul",
          borderColor: "#3cba9f"
        }, {
          data: sadWords,
          label: "sad",
          borderColor: "#8e5ea5"
        },
      ]
    },
    option: {
      title: {
        display: true,
        text: "Some title"
      }
    }
  });
</script>
</body>

</html>
